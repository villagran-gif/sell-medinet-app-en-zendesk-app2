{
  "name": "Medinet",
  "author": {
    "name": "Clinyco",
    "email": "villagran@clinyco.cl"
  },
  "defaultLocale": "en",
  "private": true,
  "frameworkVersion": "2.0",
  "location": {
    "sell": {
      "deal_card": "assets/iframe.html"
    }
  },
  "domainWhitelist": [
    "sell-medinet-backend.onrender.com"
  ],
  "parameters": [
    {
      "name": "backend_base_url",
      "type": "text",
      "required": true,
      "default": "https://sell-medinet-backend.onrender.com"
    },
    {
      "name": "backend_api_key",
      "type": "text",
      "required": true,
      "secure": true,
      "scopes": [
        "header"
      ]
    },
    {
      "name": "backend_timeout_ms",
      "type": "text",
      "required": false,
      "default": "20000"
    }
  ]
}
